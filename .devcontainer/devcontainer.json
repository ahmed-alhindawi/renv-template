{	
	"name": "Projectname",
	"build": { 
		"dockerfile" : "Dockerfile",
		"context" : ".." 
	},
	"workspaceMount": "source=${localWorkspaceFolder}, target=/workspaces, type=bind,Z",
	"workspaceFolder": "/workspaces",
	"postStartCommand": "ln -s /renv_build/renv/library/R-4.3/x86_64-pc-linux-gnu/* /workspaces/renv/library/R-4.3/x86_64-pc-linux-gnu",
	"customizations": {
			"vscode": {
				"extensions": [
					"reditorsupport.r",
					"quarto.quarto",
					"usernamehw.errorlens"
     				 ]
  			}
  	}
}
